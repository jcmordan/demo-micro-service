{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/BookingApp.Microservices.sln",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "type": "dotrush.task",
      "project": "${workspaceFolder}/RoomService/RoomService.Api/RoomService.Api.csproj",
      "problemMatcher": [],
      "label": "Build RoomService.Api"
    },
    {
      "type": "dotrush.task",
      "project": "${workspaceFolder}/UserService/UserService.Api/UserService.Api.csproj",
      "problemMatcher": [],
      "label": "Build UserService.Api"
    },
    {
      "type": "dotrush.task",
      "project": "${workspaceFolder}/BookingService/BookingService.Api/BookingService.Api.csproj",
      "problemMatcher": [],
      "label": "Build BookingService.Api"
    }
  ]
}
